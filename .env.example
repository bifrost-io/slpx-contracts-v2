# ========================================
# 多签监控配置
# ========================================

# Slack配置
SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# Slack用户ID（用于@提醒）
# 格式: SLACK_USER_ID_用户名=U1234567890
SLACK_USER_ID_ADMIN=U1234567890
SLACK_USER_ID_DEVOPS=U0987654321
SLACK_USER_ID_SECURITY=U1122334455

# 需要@的用户列表（用逗号分隔）
# 可以使用具体的用户ID，也可以使用环境变量引用
SLACK_MENTION_USERS=SLACK_USER_ID_ADMIN,SLACK_USER_ID_DEVOPS,SLACK_USER_ID_SECURITY

# 监控配置
# 是否启用Slack告警（true/false）
ENABLE_SLACK_ALERT=true

# 日志配置
LOG_LEVEL=info

# ========================================
# Hardhat RPC URL 配置
# ========================================

# 私钥（用于部署和交易）
PRIVATE_KEY=your_private_key_here

# 各网络 RPC URL
OP_URL=https://mainnet.optimism.io
ARB_URL=https://arb1.arbitrum.io/rpc
BSC_URL=https://bsc-dataseed1.binance.org
BASE_URL=https://mainnet.base.org
SONEIUM_URL=https://rpc.soneium.com

# ========================================
# Etherscan API Keys（可选）
# ========================================

# 各网络的 API Keys（用于合约验证）
ETHEREUM_API_KEY=your_ethereum_api_key
OP_API_KEY=your_optimism_api_key
ARB_API_KEY=your_arbitrum_api_key
BSC_API_KEY=your_bsc_api_key
BASE_API_KEY=your_base_api_key
MOONBEAM_API_KEY=your_moonbeam_api_key
MOONRIVER_API_KEY=your_moonriver_api_key
ASTAR_API_KEY=your_astar_api_key
SONEIUM_API_KEY=your_soneium_api_key 